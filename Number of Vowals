#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>

char word[] = "TestingA";


int count = 0;


void main()
{
    for (int i = 0; word[i] != '\0'; i++)
    {
        word[i] = tolower(word[i]);
        
        
        if (word[i] == 'a' || word[i] == 'e' || word[i] == 'i' || word[i] == 'o' || word[i] == 'u')
        {
            ++count;
        };

    };

    printf("%d", count);
    return 0;


	
}
