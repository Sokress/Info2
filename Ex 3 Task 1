
#define _CRT_SECURE_NO_WARNINGS

#include <stdio.h>

int whatDoesItDo(int A[], int n, int k)
{
	int result = -1000;
	for (int i = 1; i < n; i++)
	{
		int current = 0;
		for (int j = i; j < n; j = j + k)
		{
			current = current + A[j];
		}

		if (current > result)
		{
			result = current;
		}
	}

	return result;
}

void main()
{
	int A[] = { 1,3,5,4,2,6,8 };
	int n = 7;
	int k = 3;

	printf("The output for this example is: %d", whatDoesItDo(A, n, k));
	return 0;

}

