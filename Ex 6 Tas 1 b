// Online C compiler to run C program online
#include <stdio.h>

int main() {
    double d = 1.54;
    int i= 2;
    char ch = "d";
    
    double* p_d;
    int* p_i;
    char* p_ch;
    
    p_d = &d;
    p_i = &i;
    p_ch = &ch;
    
   printf("values: %f, %i, %c\n", d, i, ch);
   printf("addresses: %x, %x, %x\n", p_d, p_i, p_ch);
   printf("memory size: %zu, %zu, %zu\n", sizeof(d), sizeof(i), sizeof(ch));
   


    
    return 0;
}
